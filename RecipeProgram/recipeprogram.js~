recipeNames = [""];
nameArray = [""];
getNames();
makeList();

function getNames () {
	for (i = 0; i < recipes.length-1; i++) {
		newName = String(recipes[i].properName);
		recipeNames[i] = newName;
	}
}

function makeList () {
	for (i = 0; i < recipes.length-1; i++) {;
		nameArray[i] = recipeNames[i]
		if (i == 0) {
			nameList = nameArray[i];
		} else {
		nameList = nameList + "<br/>" + nameArray[i];
		}
	}
}

document.getElementById('mainDiv').innerHTML = nameList;